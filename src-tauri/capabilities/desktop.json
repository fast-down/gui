{
  "identifier": "desktop-capability",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "autostart:default",
    "updater:default",
    "process:allow-restart",
    "process:allow-exit",
    "core:app:allow-default-window-icon",
    "core:window:allow-set-focus",
    "core:window:allow-unminimize",
    "core:window:allow-show",
    "core:window:allow-hide"
  ]
}