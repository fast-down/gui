{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "fast-down-gui",
  "version": "0.1.29",
  "identifier": "com.fast-down.fast-down-gui",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist",
    "removeUnusedCommands": true
  },
  "app": {
    "windows": [
      {
        "title": "fast-down",
        "minWidth": 879,
        "width": 879,
        "minHeight": 150,
        "height": 660
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDI1RUMwNkFDQjI1MTE4NQpSV1NGRVNYTGFzQmVBbmNqQXd0OUNFNW5tQndSM1loMi9DSFNiRHhwU0RJWXVaWE1oSVp5YWpOZgo=",
      "endpoints": ["https://fast-down-update.s121.top/gui/latest"]
    },
    "deep-link": {
      "mobile": [{ "host": "fast.s121.top", "pathPrefix": ["/open"] }],
      "desktop": {
        "schemes": ["fast-down"]
      }
    }
  }
}
